00155 QString VaultBackend::generateNickname({\cf18 int} maxLength, {\cf18 bool} useMushup, {\cf18 bool} useInjection) \{\par
00156     {\cf19 if} (m_themeWords.isEmpty() || m_themeWords.keys().isEmpty()) \{\par
00157         {\cf19 return} {\cf22 "ERROR_NO_THEMES"};\par
00158     \}\par
00159 \par
00160     QStringList allThemes = m_themeWords.keys();\par
00161     QString theme1 = allThemes.at(QRandomGenerator::system()->bounded(allThemes.size()));\par
00162     QString theme2 = allThemes.at(QRandomGenerator::system()->bounded(allThemes.size()));\par
00163 \par
00164     QStringList words1 = m_themeWords.value(theme1);\par
00165     QStringList words2 = m_themeWords.value(theme2);\par
00166 \par
00167     QString baseNickname = {\cf22 ""};\par
00168 \par
00169     {\cf19 if} (useMushup && words1.size() > 0 && words2.size() > 0) \{\par
00170         QString word1 = words1.at(QRandomGenerator::system()->bounded(words1.size()));\par
00171         QString word2 = words2.at(QRandomGenerator::system()->bounded(words2.size()));\par
00172 \par
00173         {\cf18 int} split1 = QRandomGenerator::system()->bounded(word1.length() > 1 ? word1.length() : 1);\par
00174 \par
00175         baseNickname = word1.left(split1) + word2.right(word2.length() - split1);\par
00176         baseNickname = baseNickname.toLower();\par
00177 \par
00178     \} {\cf19 else} {\cf19 if} (words1.size() > 0) \{\par
00179         baseNickname = words1.at(QRandomGenerator::system()->bounded(words1.size()));\par
00180         baseNickname = baseNickname.toLower();\par
00181     \} {\cf19 else} \{\par
00182         {\cf19 return} {\cf22 "nick_error"};\par
00183     \}\par
00184 \par
00185     QString finalNickname = baseNickname;\par
00186 \par
00187     {\cf19 if} (useInjection) \{\par
00188         {\cf17 const} QString symbols = {\cf22 "!@#$%^&"};\par
00189         {\cf18 int} numSymbols = QRandomGenerator::system()->bounded(1, 4);\par
00190 \par
00191         {\cf19 for} ({\cf18 int} i = 0; i < numSymbols; ++i) \{\par
00192             {\cf18 int} pos = QRandomGenerator::system()->bounded(finalNickname.length() + 1);\par
00193             finalNickname.insert(pos, symbols.at(QRandomGenerator::system()->bounded(symbols.length())));\par
00194         \}\par
00195 \par
00196         finalNickname += QString::number(QRandomGenerator::system()->bounded(100, 999));\par
00197     \}\par
00198 \par
00199     finalNickname.replace({\cf23 ' '}, {\cf23 '_'});\par
00200 \par
00201     {\cf19 if} (finalNickname.length() > maxLength) \{\par
00202         finalNickname = finalNickname.left(maxLength);\par
00203     \}\par
00204 \par
00205     {\cf19 return} finalNickname;\par
00206 \}\par
