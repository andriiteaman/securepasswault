00106     QByteArray iv = fileData.mid(40, 8);\par
00107     QByteArray encryptedContent = fileData.mid(48, fileData.size() - 48 - 32);\par
00108 \par
00109     QByteArray key = deriveKey(masterKey, salt);\par
00110     QByteArray result;\par
